{
  "name": "@loltech/vue3-2fa-code-input",
  "version": "1.0.0",
  "description": "Vue 3 input component for 2FA codes. Is alpha-numerically agnostic, supports copy-pasting and is written in Typescript.",
  "keywords": [
    "vue",
    "vue3",
    "auth",
    "2fa",
    "code",
    "input"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "author": {
    "name": "Vlad Pascu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoLFactor/vue3-2fa-code-input"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" build-types --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-types": "vue-tsc -p tsconfig.build.json",
    "build-docs": "vite --config vite-docs.config.ts build",
    "type-check": "vue-tsc --build --force"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.18",
    "@heroicons/vue": "^2.1.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
